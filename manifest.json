{
  "name": "search-tab-crx",
  "version": "0.0.3",
  "manifest_version": 2,
  "icons": {
    "48": "pic/s.png"
  },
  "background": {
    "scripts": ["lib/bg.js"]
  },
  "options_page": "build/options.html",
  "permissions": [
    "tabs",
    "history",
    "bookmarks",
    "chrome://favicon/",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "lib/*",
    "build/*",
    "coffee/*"
  ],
  "browser_action": {
    "default_icon": "pic/s.png"
  },
  "commands": {
    "launch": {
      "suggested_key": {
        "default": "Ctrl+Period",
        "mac": "Command+Period"
      },
      "description": "Launch my searching tool"
    }
  }
}